{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Azkari Extension",
    "description": "Extension built with React and TypeScript",
    "permissions": [
      "topSites",
      "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "js/index.html"
      },
      "icons": {
        "48": "logo48.png",
        "96": "logo96.png"
      }
}
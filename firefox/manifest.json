{
    "version": "1.0.1",
    "manifest_version": 3,
    "name": "Azkari Extension",
    "description": "Update new tab page to include Muslim Zekr",
    "homepage_url": "https://github.com/azkari-webext/azkari-webext-firefox",
    "permissions": [
      "topSites",
      "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "js/index.html"
      },
      "icons": {
        "48": "js/logo48.png",
        "96": "js/logo96.png"
      },
    "browser_specific_settings": {
      "gecko": {
        "id": "azkari@webext.com"
      }
    }
}